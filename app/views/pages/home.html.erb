<div data-controller="search">
  <%= render "search_bar", categorys: @categorys%>
  <div class="container">
    <h1 class="title">Browse Rooms</h1>
    <div data-search-target="deck" class="card-deck">
      <% @rooms.each do |room| %>
        <%= render "room", room: room %>
      <% end %>
    </div>
  </div>
 </div>
